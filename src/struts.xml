<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<constant name="struts.enable.DynamicMethodInvocation" value="false" />
	<constant name="struts.devMode" value="true" />

	<package name="default" namespace="/" extends="struts-default">
		<!-- é€šç”¨action -->
		<action name="index" class="org.webapp.action.MemosAction">
			<result>/rs.jsp</result>
		</action>

		<action name="list" method="list" class="org.webapp.action.MemosAction">
			<result>/rs.jsp</result>
		</action>

		<action name="save" method="save" class="org.webapp.action.MemosAction">
			<result>/rs.jsp</result>
		</action>

		<action name="one" method="one" class="org.webapp.action.MemosAction">
			<result>/rs.jsp</result>
		</action>

		<action name="delete" method="delete" class="org.webapp.action.MemosAction">
			<result>/rs.jsp</result>
		</action>
		
		<action name="edit" method="edit" class="org.webapp.action.MemosAction">
			<result>/rs.jsp</result>
		</action>
		
		<action name="seq" method="seq" class="org.webapp.action.MemosAction">
			<result>/rs.jsp</result>
		</action>
	</package>
</struts>
